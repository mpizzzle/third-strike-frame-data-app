<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fd3s.framedata" >

    android:allowBackup="true"
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="fd3s.framedata.SelectScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="fd3s.framedata.moves.ShowMoveCategoriesActivity"
            android:label="@string/title_activity_character" >
        </activity>
        <activity
            android:name="fd3s.framedata.moves.ShowMoveListActivity"
            android:label="@string/title_activity_character" >
        </activity>
        <activity
            android:name="fd3s.framedata.moves.ShowMoveDetailActivity"
            android:label="@string/title_activity_character" >
        </activity>
        <activity
            android:name="fd3s.framedata.moves.ShowOthersActivity"
            android:label="@string/title_activity_character" >
        </activity>
        <activity
            android:name="fd3s.framedata.appmenu.UserSettingActivity"
            android:label="@string/preference_attributes" >
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
</manifest>
